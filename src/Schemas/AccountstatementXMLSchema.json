{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "since": { "type": "string", "format": "date" },
    "payout_id": { "type": "string" },
    "page": { "type": "string" },
    "per_page": { "type": "string" },
    "until": { "type": "string", "format": "date"  }
  },
  "required": ["until"],
  "oneOf": [
    { "required": ["since"] },
    { "required": ["payout_id"] }
  ]
}